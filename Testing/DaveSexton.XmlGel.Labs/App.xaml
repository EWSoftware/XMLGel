<Application x:Class="DaveSexton.XmlGel.Labs.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:labs="clr-namespace:DaveSexton.Labs;assembly=DaveSexton.Labs"
						 xmlns:local="clr-namespace:DaveSexton.XmlGel.Labs">

	<Application.Resources>

		<ResourceDictionary>
			<BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
			<labs:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter" />
			<labs:InverseBooleanConverter x:Key="InverseBooleanConverter" />
			<local:GetTypeNameConverter x:Key="GetTypeNameConverter" />
		</ResourceDictionary>

	</Application.Resources>

</Application>